                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CAST(ISNULL(NULLIF(LTRIM(RTRIM(INV_ITEM.INV_NUM)),''),'')	AS VARCHAR)				AS InvoiceNumber,
		CAST(ISNULL(NULLIF(LTRIM(RTRIM(INV_ITEM.CO_Line)),''),'')	AS VARCHAR)				AS InvoiceLineNumber,
