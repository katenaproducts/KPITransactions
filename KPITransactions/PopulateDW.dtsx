		CAST(INV.co_num as VARCHAR)															AS RMA_NUM,
		CAST(ITM.ITEM as VARCHAR)															AS ITEM
FROM				dbo.INV_ITEM_MST INV_ITEM
INNER JOIN			dbo.INV_HDR_MST  INV					ON			INV.INV_NUM  = INV_ITEM.INV_NUM
